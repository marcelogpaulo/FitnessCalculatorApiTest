  "type": "object",
  "properties": {
    "status_code": {
      "type": "integer",
      "minimum": 100,
      "maximum": 599
    },
    "request_result": {
      "type": "string"
    },
    "errors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["status_code", "request_result", "errors"],
  "additionalProperties": false
}
