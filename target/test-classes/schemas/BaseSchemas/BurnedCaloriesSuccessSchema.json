{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "status_code": {
      "type": "integer",
      "enum": [200]
    },
    "request_result": {
      "type": "string",
      "enum": ["Successful"]
    },
    "data": {
      "type": "object",
      "properties": {
        "burnedCalorie": {
          "type": "string",
          "pattern": "^[0-9]+(\.[0-9]{1,2})?$"
        },
        "unit": {
          "type": "string",
          "enum": ["calorie"]
        }
      },
      "required": ["burnedCalorie", "unit"],
      "additionalProperties": false
    }
  },
  "required": ["status_code", "request_result", "data"],
  "additionalProperties": false
}