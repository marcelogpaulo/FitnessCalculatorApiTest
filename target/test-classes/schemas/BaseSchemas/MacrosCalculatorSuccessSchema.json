              "type": "number"
            },
            "fat": {
              "type": "number"
            },
            "carbs": {
              "type": "number"
            }
          },
          "required": ["protein", "fat", "carbs"]
        },
        "lowcarbs": {
          "type": "object",
          "properties": {
            "protein": {
              "type": "number"
            },
            "fat": {
              "type": "number"
            },
            "carbs": {
              "type": "number"
            }
          },
          "required": ["protein", "fat", "carbs"]
        },
        "highprotein": {
          "type": "object",
          "properties": {
            "protein": {
              "type": "number"
            },
            "fat": {
              "type": "number"
            },
            "carbs": {
              "type": "number"
            }
          },
          "required": ["protein", "fat", "carbs"]
        }
      },
      "required": ["calorie", "balanced", "lowfat", "lowcarbs", "highprotein"]
    }
  },
  "required": ["status_code", "request_result", "data"],
  "additionalProperties": false
